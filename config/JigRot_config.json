{
"model": {
    "name": "JigRot",
    "backbone": "pre-trained",
    "load_model": "outputs/Jigsaw_base/best_model.pth",
    "num_tiles": 9,
    "num_perms": 100,
    "rotations": 4,
    "dropout": 0.5,
    "batch_norm": true,
    "tile_rotations": 4},
"data": {
    "path": "data/jersey_royals_ssl_ds",
    "tt_split": 0.8,
    "tv_split": 0.8,
    "train_loader": {
        "batch_size": 4,
        "shuffle": true,
        "num_workers": 8
    },
    "test_loader": {
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 8
    },
    "validation_loader": {
        "batch_size": 1,
        "shuffle": false,
        "num_workers": 8
        }
    },
"optimiser": {
    "name": "Adam",
    "lr": 0.00005
    },
"loop":{
    "start_epoch": 0,
    "end_epoch": 50
    },
"logging": {
    "experiment_name": "JigRot_test",
    "print_freque": 20
    }
}